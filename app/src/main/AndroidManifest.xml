<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tristate.gasper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.tristate.gasper.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tristate.gasper.SignInActivity"
            android:exported="false"
            android:parentActivityName="com.tristate.gasper.StartActivity" />
        <activity
            android:name="com.tristate.gasper.SignUpActivity"
            android:exported="false"
            android:parentActivityName="com.tristate.gasper.StartActivity" />
        <activity
            android:name="com.tristate.gasper.MainActivity"
            android:exported="false" />
    </application>

</manifest>